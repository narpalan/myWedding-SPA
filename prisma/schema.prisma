generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model contact_data {
  id         Int      @id @default(autoincrement())
  name       String
  email      String
  created_at DateTime @default(now())
}

model interested_person {
  id         Int      @id @default(autoincrement())
  name       String
  email      String
  mobile     String
  created_at DateTime @default(now())
}

model RSVP {
  id          Int       @id @default(autoincrement())
  fullName    String
  mobile      String
  created_at  DateTime  @default(now())

  @@map(name: "rsvp")
}